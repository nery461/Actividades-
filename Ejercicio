#include <stdio.h>

float máximo(float a, float b) {
    float mayor;
    a = a  ;
   rr b = b  ;
    mayor = (a > b) ? a : b;
    return mayor;
}

int main() {
    float  a, b, resultado;

    printf("número uno: ");
    scanf("%f", &a);

    printf("numero 2: ");
    scanf("%f", &b);

    resultado = máximo(a, b);

    printf(" mayor es: %2.f\n", resultado);

    return 0;
