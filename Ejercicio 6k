#include <stdio.h>
#include <stdlib.h> 

int main (){
        //Quiero imprimir el número 5
        printf( "%d \n" , 5);
        
        //Quiero imprimir la suma de 5 más 8
        printf("%d \n", 3+4);
        
        printf("La suma de %d + %d es = %d\n", 5,8, 5+8);
        
        //Quiero imprimir el número 78787
        printf( "%d \n" , 78787);
        
        //Quiero imprimir la suma de 78787 más 3259
        printf("%d \n", 78787+3259);
        
        printf("La suma de %d + %d es = %d\n", 78787,3259, 78787+3259);
        
        system("pause");
        return 0;
}dd
